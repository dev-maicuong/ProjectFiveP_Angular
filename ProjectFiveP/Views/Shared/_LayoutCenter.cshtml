                                <!-- Card body -->
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            <h5 class="card-title text-uppercase text-muted mb-0">Performance</h5>
                                            <span class="h2 font-weight-bold mb-0">49,65%</span>
                                        </div>
                                        <div class="col-auto">
                                            <div class="icon icon-shape bg-gradient-info text-white rounded-circle shadow">
                                                <i class="ni ni-chart-bar-32"></i>
                                            </div>
                                        </div>
                                    </div>
                                    <p class="mt-3 mb-0 text-sm">
                                        <span class="text-success mr-2"><i class="fa fa-arrow-up"></i> 3.48%</span>
                                        <span class="text-nowrap">Since last month</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
    <!-- Argon Scripts -->
    <!-- Core -->
    <script src="~/Content/LayoutCenter/vendor/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/LayoutCenter/vendor/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/LayoutCenter/vendor/js-cookie/js.cookie.js"></script>
    <script src="~/Content/LayoutCenter/vendor/jquery.scrollbar/jquery.scrollbar.min.js"></script>
    <script src="~/Content/LayoutCenter/vendor/jquery-scroll-lock/dist/jquery-scrollLock.min.js"></script>
    <!-- Optional JS -->
    <script src="~/Content/LayoutCenter/vendor/chart.js/dist/Chart.min.js"></script>
    <script src="~/Content/LayoutCenter/vendor/chart.js/dist/Chart.extension.js"></script>
    <!-- Argon JS -->
    <script src="~/Content/LayoutCenter/js/argon.js?v=1.2.0"></script>
